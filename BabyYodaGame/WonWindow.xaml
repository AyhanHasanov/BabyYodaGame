<Window x:Class="BabyYodaGame.WonWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BabyYodaGame"
        mc:Ignorable="d"
        Title="WonWindow" Height="400" Width="550">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Resources/wonUI.png"/>
        </Grid.Background>

        <Canvas>
            <Border BorderBrush="#FFB86F50" BorderThickness="2.25" CornerRadius="10" Canvas.Left="196" Canvas.Top="310" HorizontalAlignment="Center" VerticalAlignment="Top" Width="158" Height="26">
                <Button Name="playBttn" Width="70" Height="22" Content="Play Again" FontFamily="Palatino Linotype" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" Canvas.Left="190" Canvas.Top="210" Background="{x:Null}" BorderBrush="{x:Null}" RenderTransformOrigin="0.639,1.646" Click="playBttn_Click" Foreground="#FF782D2D"/>
            </Border>
        </Canvas>
    </Grid>
</Window>
